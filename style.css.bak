* {margin:0;padding:0;}
.cf:after {visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}
.cf {display:inline-block;}
* html .cf {height:1%;}
.cf {display:block;zoom:1;}
nav,footer,header {display:block;}
::-moz-selection { background: #333; color: #fff; text-shadow: none; }
::selection { background: #333; color: #fff; text-shadow: none; }
body {
	font-size: 1.15em;
	font-family: Constantia, "Lucida Bright", Lucidabright, "Lucida Serif", Lucida, "DejaVu Serif", "Bitstream Vera Serif", "Liberation Serif", Georgia, serif;
	color: #111;
	border-top: 5px solid #111;
	padding: 0 0 2em;
}
a {
	text-decoration: none;
	color: #E47911;
	transition: color 0.25s;
	-moz-transition:color 0.25s;
	-webkit-transition:color 0.25s;
	-o-transition:color 0.25s;
}
a.bookLink {
	color: #111;
	text-decoration: none;
	font-style: italic;
}
a.bookLink:hover, a.bookLink:focus {
	text-decoration: underline;
}
.hidden {
	display: none;
}
.title {
	font-weight: bold;
	font-size: 1.1em;
	padding: 0.5em 0 0.75em;
	text-transform: uppercase;
	font-family: 'Krona One';
	color: #E87D03;
}
#blog .title {
	padding: 0 0 0.1em;
}
.footnote {
	color: #999;
	font-style: italic;
}
.footnote .back {
	float: left;
}
.footnote .forward {
	float: right;
}
.footnote .archives {
	padding-top: 1em;
	display: block;
	text-align: center;
}
h2 {
	margin: 0 0 .5em;
}
.borderAfter:after {
	display: block;
	content: '';
	border-bottom: 1px solid #111;
	margin: 1.5em auto;
	width: 50%;
}
#tshirt {
	
}
#buy {
	font-size: 1.25em;
	padding: 0 0 .5em;
	position: relative;
	overflow: auto;
	cursor: pointer;
	color: #E47911;
}
#buy > .buyLinks {
	font-size: .75em;
	cursor: text;
	padding: .5em 0 0;
	color: #111;
}
#buy .buyLinks a {
	text-decoration: underline;
}
#buy:hover > .buyLinks, #buy:focus > .buyLinks {
	display: block;
}
h3 {
	margin: 0 0 0.5em;
	font-size: 1em;
	color: #333;
}
#page {
	width: 960px;
	margin: auto;
}
#header {
	position: relative;
	padding: 1.5em 0 0;
}
#logo {
	font-size: 2.25em;
	line-height: 1em;
	padding: 0.1em 0 0 0.1em;
	text-align: center;
	margin: 0 0 0.2em;
	text-transform: uppercase;
	font-family: 'Krona One',sans-serif;
}
#logo .first {
}
#logo .last, #logo .creds {
}
#logo .creds {
}
#logo a {
	color: #E47911;
	text-shadow: 1px 1px 1px rgba(0,0,0,0.2);
	text-decoration: none;
}
#logo a:hover, #logo a:focus {
	color: #666;
}
nav, #nav a {
	border: solid #333;
	text-decoration: none;
	cursor: pointer;
}
nav {
	font-family: 'Tahoma','trebuchet ms',sans-serif;
	border-width: 1px 0;
}
#nav ul {
	-moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;
}
#nav li {
	list-style: none;
	float: left;
	height: 100%;
	padding: 0.5em 0;
	position: relative;
	width: 16.6%;
}
#nav a {
	font-size: 1em;
	display: block;
	text-align: center;
	color: #111;
	height: 100%;
	text-transform: uppercase;
	border-width: 0 1px 0 0;
	transition: color 0.25s;
	-moz-transition:color 0.25s;
	-webkit-transition:color 0.25s;
	-o-transition:color 0.25s;
}
#nav a.last {
	border: 0;
}
#nav a:hover, #nav a:focus, #nav a.active {
	color: #E47911;
}
#nav a.active {
	font-weight: bold;
}
#nav .sub {
	position: absolute;
	padding: 0.5em 0 0;
	top: 1.6em; left: -1px; right: 0;
	display: none;
	background: #fff;
	border: 1px solid #111;
	border-width: 0 1px 1px;
}
#nav li.resources:hover .sub {
	display: block;
}
#nav .sub li {
	float: none;
	width: auto;
}
#nav .sub li a {
	border: 0;
	text-align: center;
}
#sidebar {
	width: 270px;
	float: right;
	border: solid #111;
	border-width: 0 0 0 1px;
	padding: 1em 0 0;
}
.framed {
	background: #fff;
	padding: 5px;
	border: 1px solid #ccc;
	color: #ccc;
}
#sidebar .framed {
	margin: 0 auto 2em;
	display: block;
}
#main {
	float: left;
	width: 660px;
	padding: 1em 1em 1em 0;
}
#theStuff {
	padding: 1em 1em 1em 0;
}
a {
	text-decoration: none;
}
a:hover, a:focus {
	text-decoration: underline;
}
p {
	margin: 0 0 1.5em;
	line-height: 1.7em;
}
p:last-child {
	margin-bottom: 0;
}
.imageLeft {
	float: left;
	margin: 0 1em 0.5em 0;
}
.imageRight {
	float: right;
	margin: 0 0 0.5em 0.75em;
}
#footer {
	color: #333;
	font-style: italic;
	font-size: 0.75em;
	text-align: center;
	border-top: 1px solid #111;
	padding: 1em 0 0;
}
#footer a {
}
#footer a:hover, footer a:focus {
	text-decoration: underline;
}
#footer .sep {
	color: #aaa;
	padding: 0 0.25em;
}
#footer .links {
	padding: 0.5em 0 0;
	font-size: 0.75em;
	font-style: normal;
}
#footer p {
	margin: 0 0 0.5em;
}
#footer .blurb {
	font-size: 0.75em;
}
#twitterBox {
	width: 310px;
	margin: 0 auto;
}
#twitterBox h3 {
	border-bottom: 1px solid #111;
}
#twitterBox h3 span {
	float: left;
	background: #fff;
	padding: 2px 10px 2px 0;
}
#twitterBox h3 a {
	text-decoration: none;
}
#twitterBox h3 a:hover {
	text-decoration: underline;
}
#twitterBox .twtr-hd, #twitterBox .twtr-ft {
	display: none;
}
#twitterBox .twtr-tweet-wrap {
	padding-left: 0;
}
.post {
	margin: 0 0 2.5em;
	padding: 0 0 1em;
}
.post:last-child {
	margin-bottom: 0;
}
.post .byline .date {
	display: block;
	margin: 0 0 0.5em;
}
.post .byline .share {
	display: block;
}
.post .byline .social {
	float: left;
}
.post .byline .share .pinterest {
	width: 90px;
}
.post .byline .share .facebook {
	width: 100px;
}
.post p {
	margin: 0 0 1em;
}
.post :last-child {
	margin-bottom: 0;
}
.post .title {
	font-size: 1.25em;
}
.post .title a {
	text-decoration: none;
}
.post .byline {
	font-size: 0.75em;
	font-style: italic;
	margin: 0 0 0.75em;
}
#main ol, #main ul {
	margin: 0 0 1em 2em;
}
#archives #main ul {
	margin-left: 4em;
}
#archives #main h3 {
	margin-left: 1em;
}
#main li {
	margin: 0 0 0.5em;
}
.row {
	margin: 0 0 1em;
}
.row label {
	display: block;
}
.row input, .row textarea {
	border: 1px solid #111;
	padding: 0.25em;
}
.row input {
	width: 25%;
}
.row textarea {
	width: 75%;
	height: 20em;
}
blockquote {
	margin: 1em;
	padding: .5em 1em;
	border-left: 5px solid #aaa;
	background: #eee;
	font-style: italic;
}
.cheat {
	border: 1px solid #ddd;
	padding: 0.5em 1em;
	color: #666;
}
.cheat th {
	text-align: left;
	text-decoration: underline;
	padding: 0 0 0.25em;
}
.cheat td {
	padding: 0.75em 1em 0 0;
	white-space: nowrap;
}
input.txt, textarea {
	display: block;
	border: 1px solid #ccc;
	padding: 0.25em;
	margin: 0.25em 0 1.25em;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	background: #f5f5f5;
	width: 30%;
	-moz-box-shadow: inset 0px 2px 2px rgba(0,0,0,0.1);
	-webkit-box-shadow: inset 0px 2px 2px rgba(0,0,0,0.1);
}
textarea {
	width: 50%;
}
input.txt:focus, textarea:focus {
	background: #fff;
	-moz-box-shadow: 0px 2px 2px rgba(0,0,0,0.2);
	-webkit-box-shadow: 0px 2px 2px rgba(0,0,0,0.2);
}
label.error {
	font-size: 0.7em;
	color: red;
	position: relative;
	z-index: 9000;
	display: none;
}
#events #main ul {
	padding: 1em 0 0;
	list-style: none;
	margin: 0;
}
#events #main li {
	margin: 0 0 2.5em;
}
#events #main li .name {
	font-size: 1.2em; 
	margin: 0;
}
#events #main h4 {
	font-size: 1em;
	font-weight: normal;
}
#events #main li .date {
	margin: 0.25em 0 0.5em;
	font-style: italic;
	color: #666;
	font-size: 0.9em;
}
#events #main p {
	font-size: .8em;
	padding: 0 0 0 1em;
	margin: 0 0 0 0.25em;
	border-left: 1px solid #ddd;
}
	.showHide {
		display: none;
	}
#contactArea .represent {
	padding: 1em 0 0;
	margin: 0 0 1em;
}
#contactArea .represent h3 {
	margin: 0 0 5px;
}
#contactArea .represent h4 {
	font-weight: normal;
	margin: 0 0 3px;
}
@media (max-width:960px) {
	.showHide {
		text-align: center;
		display: block;
		padding: .5em 0;
	}
	.showHide a {
		display: inline-block;
		background: #ddd;
		border: 1px solid #aaa;
		border-radius: 2px;
		color: #333;
		padding: 0 .5em;
	}
	.showHide a:hover, .showHide a:focus {
		text-decoration: none;
	}
	#nav {
		display: none;
	}
	#page { width: 90%; }
	#sidebar .framed {
		width: 90%;
	}
	#logo {
		font-size: 1.5em;
	}
	#main,#sidebar { float: none; width: auto; border: none; padding: 0; margin: auto; }
	#sidebar {
		width: 300px;
	}
	#nav li {
		float: none;
		width: auto;
	}
	#nav .sub {
		border: 0;
		width: auto;
		position: relative;
		display: block;
		top: auto; left: auto;
		padding: 0;
	}
	#nav .resources {
		padding: 0;
	}
	#nav .resources > a {
		display: none;
	}
	#nav a {
		width: auto;
		border: none;
	}
	.imageLeft, .imageRight {
		float: none;
		display: block;
		margin: 1em auto 1em;
	}
	#footer span {
		display: block;
	}
#twitterBox {
	width: 100%;
}
}
